{
  "compilerOptions": {
    "target": "es2022",
    "module": "commonjs",
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "allowJs": false,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowUnusedLabels": false
  },
  "include": ["src"]
}